@model DDX.OrderManagementSystem.Domain.ProjectType

@{
   Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>DDX.OrderManagementSystem.Domain.ProjectType</title>
</head>
<body>
<fieldset>
    <legend>DDX.OrderManagementSystem.Domain.ProjectType</legend>
    <div class="display-label">主键标识</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">项目标题</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Title)
    </div>

    <div class="display-label">项目内容</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Content)
    </div>

    <div class="display-label">项目状态</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.State)
    </div>

    <div class="display-label">项目备注</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Remark)
    </div>

    <div class="display-label">创建人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateBy)
    </div>

    <div class="display-label">创建时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateOn)
    </div>

    <div class="display-label">审核人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AuditBy)
    </div>

    <div class="display-label">审核时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AuditOn)
    </div>

    <div class="display-label">审核备注</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AuditRemark)
    </div>

    <div class="display-label">最近开发进度</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LastState)
    </div>

    <div class="display-label">最近更新时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LastOn)
    </div>

    <div class="display-label">审核状态</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IsAudit)
    </div>

    <div class="display-label">开始时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BeginDate)
    </div>

    <div class="display-label">结束时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.EndDate)
    </div>

    <div class="display-label">评价</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.EvaluateContent)
    </div>


</fieldset>
<p>
    @Html.ActionLink("编辑", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("返回列表", "Index")
</p>
</body>
</html>
