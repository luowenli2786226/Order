    //点击设定动作的时候执行的方法
    $("input[name=radioaction]").click(function () {
        var value = $(this).val();
        if (value == "分配发货仓库") {
            $("#UnChooseCondition input[type=checkbox]:eq(0)").attr("disabled", 'disabled');
            $("#ShowActionWare").show();
            $("#ShowActionLogic").hide();
        }
        else if (value == "匹配邮寄方式") {
            $("#UnChooseCondition input[type=checkbox]:eq(0)").attr("disabled", false);
            $("#ShowActionLogic").show();
            $("#ShowActionWare").hide();
        }
        else {
            $("#ShowActionWare").hide();
            $("#ShowActionLogic").hide();
        }
    })
    //获取可选条件
    function ShowUnChooseCondition() {
        $.ajax({
            type: "post",
            url: '@Url.Action("GetUnChooseCondition", "OrdersRuler")',
            data: "",
            datatype: "json",
            success: function (msg) {
                var str = "<ul>";
                $.each(msg.data, function (ids, obj) {
                    str += "<li  >" + obj.FullName + "</li>";
                    $.each(obj.children, function (ids, obj2) {
                        if (obj2.FullName == "发货仓库为指定仓库")
                        {
                            str += "<li ><input type='checkbox' disabled='disabled' onclick=\"copy('" + obj2.Id + "','" + obj2.FullName + "')\" id='chk" + obj2.Id + "'/>" + obj2.FullName + "</li>";

                        }
                        else
                        {
                            str += "<li ><input type='checkbox'  onclick=\"copy('" + obj2.Id + "','" + obj2.FullName + "')\" id='chk" + obj2.Id + "'/>" + obj2.FullName + "</li>";

                        }
                    })
                })
                str += "</ul>";
                $("#UnChooseCondition").html(str);
            }

        });

    }
    //将可选条件添加到已选条件
    function copy(id, name) {
        var str = $("#txt").html();
        //当前checkbox选中的时候，增加一个条件可选
        if ($('#chk' + id).is(':checked')) {
            var splitname = name.split('指定');
            if (splitname.length == 2) {
                name = '<div id=div' + id + '>' + splitname[0] + '<a href="#" id=a' + id + ' onclick="ShowCondition(\'' + id + '\',\'' + name + '\')">指定' + splitname[1] + '</a></div>';
            }
            else if (splitname.length == 3) {
                name = '<div id=div' + id + '>' + splitname[0] + '<a href="#" id=a' + id + ' onclick="ShowCondition(\'' + id + '\',\'' + name + '\')">指定' + splitname[1] +'指定'+splitname[2]+ '</a></div>';
            }
            str = str + name;
            //$("#txt").append(str);
            $("#txt").append(name);
        }
        else {
            //移除这个可选条件
            $("#div" + id).remove();
        }

    }
