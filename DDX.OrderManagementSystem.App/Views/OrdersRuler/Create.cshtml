    //显示对话框
    function ShowDialog(url, width, height, title, handle, id) {
        $(url).dialog({
            width: width,
            hight: height,
            title: title,
            modal: true,
            loadingMessage: '正在加载...',
            buttons: [{
                text: '确定',
                handler: handle
            }, {
                text: '取消',
                handler: function () {
                    $(url).dialog('close');
                }
            }]
        }).dialog('open');
    }
