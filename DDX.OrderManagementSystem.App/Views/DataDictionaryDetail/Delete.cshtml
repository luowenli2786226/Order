@model DDX.OrderManagementSystem.Domain.DataDictionaryDetailType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>DDX.OrderManagementSystem.Domain.DataDictionaryDetailType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>DDX.OrderManagementSystem.Domain.DataDictionaryDetailType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">分类代码</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.DicCode)
    </div>

    <div class="display-label">名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FullName)
    </div>

    <div class="display-label">值</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.DicValue)
    </div>

    <div class="display-label">内置</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AllowDelete)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
