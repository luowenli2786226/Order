    <div class="display-label">仓库</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.WName)
    </div>

    <div class="display-label">入库数量</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Qty)
    </div>

    <div class="display-label">现有数量</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NowQty)
    </div>

    <div class="display-label">备注</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Remark)
    </div>

    <div class="display-label">运费</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Freight)
    </div>

    <div class="display-label">金额</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Amount)
    </div>

    <div class="display-label">生产日期</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ProductionOn)
    </div>

    <div class="display-label">到期时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ExpirationOn)
    </div>

    <div class="display-label">入库类型</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.InType)
    </div>

    <div class="display-label">创建人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateBy)
    </div>

    <div class="display-label">创建时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateOn)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
