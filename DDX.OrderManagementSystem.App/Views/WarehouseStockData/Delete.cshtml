@model HanRuiOMS.Domain.WarehouseStockDataType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>HanRuiOMS.Domain.StockInDataType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>HanRuiOMS.Domain.StockInDataType</legend>
    <div class="display-label">标识</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">入库ID</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.InId)
    </div>

    <div class="display-label">入库序列号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.InNo)
    </div>

    <div class="display-label">入库状态</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.State)
    </div>

    <div class="display-label">产品ID</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PId)
    </div>

    <div class="display-label">产品SKU</div>
